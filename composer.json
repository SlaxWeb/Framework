{
    "name": "slaxweb/framework",
    "description": "SlaxWeb/Framework, a modular Framework where you decide what you use, and how you use it",
    "license": "MIT",
    "version": "0.3.0",
    "authors": [
        {
            "name": "Tomaz Lovrec",
            "email": "tomaz.lovrec@gmail.com"
        }
    ],
    "require": {
        "php":  ">=7.0.0",
        "slaxweb/bootstrap": "0.3.*@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\Controller\\": "app/Controller/",
            "App\\Hook\\": "app/Hook/",
            "App\\Library\\": "app/Library/",
            "App\\Model\\": "app/Model/",
            "App\\Provider\\": "app/Provider/",
            "App\\Routes\\": "app/Routes",
            "App\\View\\": "app/View/"
        }
    },
    "minimum-stability": "dev"
}
